@charset "gb2312";
/* CSS Document */
#zaowanbao-pop{background:url(http://d5.sina.com.cn/litong/zhitou/sinaads/demo/wanglt/zaowanbao/pop_bg.png) no-repeat left top #fff !important;_filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true, sizingMethod=noscale, src="http://d5.sina.com.cn/litong/zhitou/sinaads/demo/wanglt/zaowanbao/pop_bg.png");_background:none; width: 320px; height: 120px;position: relative;}
#zaowanbao-pop-box{overflow: hidden; height: 120px}
.zaowanbao-pop-title{padding:7px 0 2px 17px;overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}


.zaowanbao-pop-content{height:120px;}
.zaowanbao-pop-pic{width:127px;height:114px;float:left;padding:5px 0 0 6px;}
.zaowanbao-pop-pic a{display:block;}
.zaowanbao-pop-pic img{border:1px solid #909090;}
.zaowanbao-pop-pic ins{margin:0;position:relative;}
.zaowanbao-pop-txt{width:120px;height:12px;line-height:12px;color:#112e66;font-size:12px !important;text-align:center;position:absolute;top:94px;}
.zaowanbao-pop-pic a:hover .zaowanbao-pop-txt{color:#0097e0 !important;}

.zaowanbao-pop-list{float:left;width:187px;height:120px;}
.zaowanbao-pop-list ul li{padding-left:8px;font-size:12px !important;height:21px;line-height:21px;background:url(http://d5.sina.com.cn/litong/zhitou/sinaads/demo/wanglt/zaowanbao/pop_dotted.png) no-repeat left center;overflow: hidden; text-overflow: ellipsis; white-space: nowrap; }

/*Á´½ÓÑÕÉ«*/
#zaowanbao-pop a{color: #112e66 !important;}
#zaowanbao-pop .zaowanbao-pop-title a{font-size:14px !important;color:#c2222c !important;font-weight:600;}
#zaowanbao-pop a:hover {color:#0097e0 !important;text-decoration:none;}

/*#zaowanbao-pop .zaowanbao-linkRedBox a:visited{color:#c2222c !important;}*/
#zaowanbao-pop .zaowanbao-linkRedBox a:hover{color:#0097e0 !important;}